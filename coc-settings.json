  },
  "[astro]":{
    "coc.preferences.formatOnSave": true,
  },
  "prettier.requireConfig": true,
  "prettier.disableLanguages": ["javascript", "typescriptreact", "typescript", "javascriptreact"],
  "Lua.diagnostics.globals": ["vim"]
}
